<p id="notice"><%= notice %></p>

<h1>Movieratings</h1>

<table>
  <thead>
    <tr>
      <th>Movieratingid</th>
      <th>Themovieid</th>
      <th>Rating</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movieratings.each do |movierating| %>
      <tr>
        <td><%= movierating.movieratingid %></td>
        <td><%= movierating.themovieid %></td>
        <td><%= movierating.rating %></td>
        <td><%= movierating.name %></td>
        <td><%= link_to 'Show', movierating %></td>
        <td><%= link_to 'Edit', edit_movierating_path(movierating) %></td>
        <td><%= link_to 'Destroy', movierating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movierating', new_movierating_path %>
